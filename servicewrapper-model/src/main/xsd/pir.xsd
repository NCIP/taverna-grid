<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="gme://PIR 1.2.caBIG/1.2/edu.georgetown.pir.domain" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pir="gme://PIR 1.2.caBIG/1.2/edu.georgetown.pir.domain" xmlns:irwg="http://www.mygrid.org.uk/2009/cagrid/servicewrapper/service/IRWG" elementFormDefault="qualified">
