<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.sf.taverna.t2</groupId>
  <artifactId>cagrid-activity-ui</artifactId>
  <name>Taverna 2 caGrid Activity UI.</name>
  <version>0.3-SNAPSHOT</version>
  <build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
  
  <repositories>
    <repository>
      <releases />
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>mygrid-repository</id>
      <name>myGrid Repository</name>
      <url>http://www.mygrid.org.uk/maven/repository</url>
    </repository>
    <repository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots />
      <id>mygrid-snapshot-repository</id>
      <name>myGrid Snapshot Repository</name>
      <url>http://www.mygrid.org.uk/maven/snapshot-repository</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>net.sf.taverna.t2.workbench</groupId>
      <artifactId>activity-palette-api</artifactId>
      <version>0.1</version>
    </dependency>
    <dependency>
      <groupId>net.sf.taverna</groupId>
      <artifactId>wsdl-generic</artifactId>
      <version>1.2</version>
    </dependency>
    <dependency>
      <groupId>net.sf.taverna.t2.workbench</groupId>
      <artifactId>contextual-views-impl</artifactId>
      <version>0.1</version>
    </dependency>
    <dependency>
      <groupId>net.sf.taverna.t2.workbench</groupId>
      <artifactId>contextual-views-api</artifactId>
      <version>0.1</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.4</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>net.sf.taverna.t2.workbench</groupId>
      <artifactId>activity-palette-impl</artifactId>
      <version>0.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>net.sf.taverna.t2</groupId>
      <artifactId>wsdl-activity</artifactId>
      <version>0.3</version>
    </dependency>
    <dependency>
      <groupId>caGrid</groupId>
     <artifactId>caGrid-package</artifactId>
      <version>1.2</version>   		
    </dependency>
  </dependencies>
</project>